{
    "autoload": {
        "psr-4": {
            "App\\": "inc/"
        },
        "files": [
            "inc/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "../tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "ext-gettext": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-soap": "*",
        "ext-xml": "*",
        "GuzzleHttp/Psr7": "^1.4",
        "ajenbo/image.php": "^0.9.3",
        "ajenbo/imap.php": "^0.9.1",
        "ezyang/htmlpurifier": ">=4.4",
        "james-heinrich/getid3": "^1.9",
        "phpmailer/phpmailer": ">=5.0.0",
        "sentry/sentry": "^1.8",
        "symfony/http-foundation": "^4.0",
        "tecnickcom/tcpdf": "^6.2",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "codacy/coverage": "^1.0",
        "mockery/mockery": "^1.0.0",
        "php-coveralls/php-coveralls": "^2.0",
        "phpstan/phpstan": "^0.10",
        "phpstan/phpstan-mockery": "^0.10.2",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
